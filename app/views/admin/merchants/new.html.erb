<%= form_with model: [:admin, @merchant], class: "merchant_form", local: true do |f| %>
<pre>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.hidden_field :status, value: 'disabled' %>
  <%= f.submit 'Submit' %>
</pre>
<% end %>

